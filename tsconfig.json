{
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "noImplicitAny": true,
        "outDir": "./build/",
        "preserveConstEnums": true,
        "removeComments": true,
        "allowJs": true,
        "typeRoots": [
            "node_modules/@types/"
        ],
        "types": [
            "react",
            "react-dom"
        ],
        "target": "ES5"
    },
    "exclude": [
        "node_modules",
        "typings/browser.d.ts",
        "typings/browser"
    ]
}
